                "remove_duplicates",
                "lowercase",
                "asciifolding"
            ],
            "tokenizer": "hebrew",
            "type": "custom"
        },
        "text_search": {
            "char_filter": [
                "globo_norm",
                "acronym_fixer",
                "camelCase_splitter",
                "word_break_helper",
                "arabic_extended_norm",
                "invis_cleanup"
            ],
            "filter": [
                "homoglyph_norm",
