            "tokenizer": "no_splitting",
            "type": "custom"
        },
        "trigram": {
            "filter": [
                "lowercase"
            ],
            "tokenizer": "trigram",
            "type": "custom"
        },
        "trigram_anchored": {
            "char_filter": [
                "add_regex_start_end_anchors"
            ],
            "filter": [
                "lowercase"
            ],
            "tokenizer": "trigram",
            "type": "custom"
        },
        "word_prefix": {
            "filter": [
                "icutokrep_no_camel_split",
                "lowercase",
                "prefix_ngram_filter"
            ],
            "tokenizer": "textify_icu_tokenizer",
            "type": "custom"
